package freeboard;

import java.io.Serializable;

public class freeboardDto implements Serializable {
	
	int seq;			// 글 고유번호
	String imageName;	// 이미지 작성
	String title;		// 글제목
	String id;			// 작성자 아이디
	String name;		// 작성자 이름
	String readcount;	// 조회수
	String wdate;		// 작성일
	String content;		// 글 내용
	String animal_kind;	// 동물 종류

}
