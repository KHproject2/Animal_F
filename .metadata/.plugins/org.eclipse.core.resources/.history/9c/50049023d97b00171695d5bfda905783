package freeboard;

import java.io.Serializable;

/*
	CREATE TABLE FBCOMMENT(
		SEQ NUMBER(8) PRIMARY KEY,
		ID VARCHAR2(50) NOT NULL,
		NAME VARCHAR2(50) NOT NULL,
		COMMENT VARCHAR2(2000),
		PNUM NUMBER(8) NOT NULL
		
	)

 */
public class fbcommentDto implements Serializable {
	
	int seq;			// 댓글 고유번호
	String id;			// 작성자 아이디
	String name;		// 작성자 이름
	String comment;		// 작성된 댓글	
	int pnum;			// 해당하는 게시글 고유번호
	
	public fbcommentDto() {
	}


	public fbcommentDto(String id, String name, String comment, int seq, int pnum) {
		this.id = id;
		this.name = name;
		this.comment = comment;
		this.seq = seq;
		this.pnum = pnum;
	}
	
	public fbcommentDto(String id, String name, String comment, int pnum) {
		this.id = id;
		this.name = name;
		this.comment = comment;
		this.pnum = pnum;
	}
	
	
}
